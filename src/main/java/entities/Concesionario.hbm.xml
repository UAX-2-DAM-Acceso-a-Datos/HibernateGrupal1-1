<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="entities.Vehiculo" table="vehiculo">
		<property name="matricula" type="string" column="matricula"/>
		<property name="marca" type="string" column="marca"/>
		<property name="modelo" type="string" column="modelo"/>
		<property name="codigo_revisiontecnica " type="int" column="codigo_revisiontecnica "/>
		
	</class>
	
	<class name="entities.RevisionTecnica" table="revisiontecnica">
		<property name="matricula" type="string" column="matricula"/>
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="calificacion" type="double" column="calificacion "/>
		
	</class>
		
</hibernate-mapping>